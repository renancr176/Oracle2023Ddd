{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TMS": "Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=$TMS_DB_HOST)(PORT=$TMS_DB_PORT))(CONNECT_DATA=(SERVER=DEDICATED)(SERVICE_NAME=$TMS_DB_SERVICE_NAME)));User Id=$TMS_DB_USER;Password=$TMS_DB_PASSWORD;Validate Connection=true;Max Pool Size=200"
  }
}